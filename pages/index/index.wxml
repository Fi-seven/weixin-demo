<!--index.wxml-->
<view class="customSwiper">
  <swiper class='banner' autoplay='true' interval='5000' duration='300'>
    <block wx:for="{{sliderList}}" wx:key="key">
      <swiper-item>
        <image style='width:100%;height:100%;' src="{{item.imageSource}}"></image>
      </swiper-item>
    </block>
  </swiper>
  
</view>
